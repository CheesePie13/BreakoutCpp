{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "shell",
			"label": "Build Windows Debug",
			"command": "win-build-debug.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			}
		},
		{
			"type": "shell",
			"label": "Build Windows Release",
			"command": "win-build-release.bat",
			"options": {
				"cwd": "${workspaceFolder}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build"
		}
	]
}